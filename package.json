{
  "name": "@epicurrents/edf-file-loader",
  "version": "0.0.0",
  "description": "EpiCurrents EDF file loader",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node node_modules/typescript/bin/tsc && node node_modules/tsconfig-replace-paths/dist/commonjs --project tsconfig.json",
    "lint": "npm run lint:src",
    "lint:src": "node node_modules/eslint/bin/eslint src",
    "prepack": "npm run build",
    "test": "npm run test:unit",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --verbose --no-cache --runInBand"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": "./dist/types/index.d.ts",
    "./package.json": "./package.json",
    "./dist/types": "./dist/types/index.d.ts"
  },
  "imports": {
    "#root/*": "./dist/*",
    "#*": "./dist/src/*"
  },
  "files": [
    "dist/*",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/epicurrents/edf-file-loader.git"
  },
  "author": "Sampsa Lohi",
  "license": "Apache-2.0",
  "dependencies": {
    "@epicurrents/core": "^0.0.2-alpha.46",
    "asymmetric-io-mutex": "^0.6.8",
    "codecutils": "^0.1.3",
    "scoped-ts-log": "^1.1.0",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@microsoft/tsdoc": "^0.14.1",
    "@types/jest": "^27.5.2",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "circular-dependency-plugin": "^5.2.2",
    "dotenv": "^16.0.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.3.0",
    "tsconfig-replace-paths": "^0.0.14",
    "typescript": "^4.7.3"
  }
}
